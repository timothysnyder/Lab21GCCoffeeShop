@{
    ViewBag.Title = "About Our Coffee Shop";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<head>
    <link href="~/Views/Home/StyleSheet.css" rel="stylesheet" />

</head>

<p>Thanks for Stopping By! Make sure you Join our Coffee Club for Special Offers.</p>
<img src="~/Images/grand-circus-logo.jpg" />

<table class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
    </tr>

    @foreach (var c in ViewBag.ItemList)
    {
        <tr>
            <td>@c.Name</td>
            <td>@c.Decsription</td>

        </tr>
    }
</table>